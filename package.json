{
  "name": "brophylactic-bot",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:kyeotic/brophylactic-bot.git",
  "author": "Tim Kye <tim@kye.dev>",
  "license": "MIT",
  "scripts": {
    "build": "run-s build:tsc build:package",
    "build:package": "del build/ && lambda-zipper build -f dist/ \"!src/**/__tests__/**\" -o ./build/lambda.zip",
    "build:tsc": "del dist/ && tsc --project tsconfig.build.json",
    "style": "prettier  --write \"src/**/*.{ts,tsx}\"",
    "style:ci": "prettier --check \"src/**/*.{ts,tsx}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --ext .ts,.tsx",
    "check": "run-s style lint",
    "check:ci": "run-s style:ci lint"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.97",
    "@types/node": "^17.0.36",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "copyfiles": "^2.4.1",
    "del-cli": "^4.0.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "base64url": "^3.0.1",
    "nanoid": "^3.3.4",
    "node-http-error": "^2.0.0",
    "request-micro": "^1.5.1",
    "tslib": "^2.4.0",
    "url-join": "^5.0.0"
  }
}
