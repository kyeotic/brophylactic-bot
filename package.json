{
  "name": "brophylactic-bot",
  "version": "1.0.0",
  "repository": "git@github.com:kyeotic/brophylactic-bot.git",
  "author": "Tim Kye <tim@kye.dev>",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/bot",
    "build": "run-s build:tsc build:es build:package",
    "build:package": "del build/ && mkdir build/ && zip build/lambda.zip -j -r dist/ -x \"*.tsbuildinfo\"",
    "build:tsc": "del dist/ && tsc --project tsconfig.build.json",
    "build:es": "node esbuild.js",
    "style": "prettier  --write \"src/**/*.{ts,tsx}\"",
    "style:ci": "prettier --check \"src/**/*.{ts,tsx}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --ext .ts,.tsx",
    "check": "run-s style lint",
    "check:ci": "run-s style:ci lint"
  },
  "dependencies": {
    "aws4": "^1.11.0",
    "base64url": "^3.0.1",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.4",
    "encodeurl": "^1.0.2",
    "jwt-simple": "^0.5.6",
    "lambda-logger-node": "^4.0.0-7",
    "nanoid": "^3.3.4",
    "node-http-error": "^2.0.0",
    "random-seed": "^0.3.0",
    "request-micro": "^2.0.2-0",
    "seed-random": "^2.2.0",
    "tslib": "^2.4.0",
    "tweetnacl": "^1.0.3",
    "url-join": "4.x"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.97",
    "@types/aws4": "^1.11.2",
    "@types/encodeurl": "^1.0.0",
    "@types/node": "^17.0.36",
    "@types/random-seed": "^0.3.3",
    "@types/seed-random": "^2.2.1",
    "@types/url-join": "4.x",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "copyfiles": "^2.4.1",
    "del-cli": "^4.0.1",
    "discord-api-types": "^0.33.5",
    "discordeno": "^13.0.0-rc45",
    "esbuild": "^0.14.43",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "lambda-zipper": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.2"
  }
}
