{
  "name": "brophylactic-bot",
  "version": "1.0.0",
  "repository": "git@github.com:kyeotic/brophylactic-bot.git",
  "author": "Tim Kye <tim@kye.dev>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/src/main.js",
  "exports": {
    ".": "./dist/src/main.js"
  },
  "typesVersions": {
    "*": {
      "main.d.ts": [
        "dist/src/main.d.ts"
      ]
    }
  },
  "scripts": {
    "start": "ts-node src/bot",
    "build": "run-s build:tsc build:package",
    "build:package": "del build/ && lambda-zipper build -f dist/ \"!src/**/__tests__/**\" -o ./build/lambda.zip",
    "build:tsc": "del dist/ && tsc --project tsconfig.build.json",
    "style": "prettier  --write \"src/**/*.{ts,tsx}\"",
    "style:ci": "prettier --check \"src/**/*.{ts,tsx}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --ext .ts,.tsx",
    "check": "run-s style lint",
    "check:ci": "run-s style:ci lint"
  },
  "dependencies": {
    "aws4": "^1.11.0",
    "base64url": "^3.0.1",
    "camelcase-keys": "^8.0.0",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.4",
    "encodeurl": "^1.0.2",
    "jwt-simple": "^0.5.6",
    "lambda-logger-node": "^4.0.0-7",
    "nanoid": "^3.3.4",
    "node-http-error": "^2.0.0",
    "request-micro": "^2.0.2-0",
    "seed-random": "^2.2.0",
    "snakecase-keys": "^5.4.2",
    "tslib": "^2.4.0",
    "tweetnacl": "^1.0.3",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.97",
    "@types/aws4": "^1.11.2",
    "@types/encodeurl": "^1.0.0",
    "@types/node": "^17.0.36",
    "@types/seed-random": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "copyfiles": "^2.4.1",
    "del-cli": "^4.0.1",
    "discord-api-types": "^0.33.5",
    "discordeno": "^13.0.0-rc45",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.2"
  }
}
