{
  "name": "discord-bot",
  "version": "1.0.0",
  "repository": "git@github.com:kyeotic/discord-bot.git",
  "author": "Tim Kye <tim@kye.dev>",
  "license": "MIT",
  "scripts": {
    "start": "npx tsx src/bot",
    "start:server": "npx tsx src/server",
    "build": "run-s build:tsc build:es",
    "build:tsc": "del dist/ && tsc --project tsconfig.build.json",
    "build:es": "node esbuild.js",
    "style": "prettier  --write \"src/**/*.{ts,tsx}\"",
    "style:ci": "prettier --check \"src/**/*.{ts,tsx}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --ext .ts,.tsx",
    "check": "run-s style lint",
    "check:ci": "run-s style:ci lint"
  },
  "dependencies": {
    "base64url": "^3.0.1",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.4",
    "encodeurl": "^1.0.2",
    "jwt-simple": "^0.5.6",
    "nanoid": "^3.3.4",
    "node-http-error": "^2.0.0",
    "pino": "^10.3.0",
    "random-seed": "^0.3.0",
    "request-micro": "^2.0.2-0",
    "seed-random": "^2.2.0",
    "tslib": "^2.4.0",
    "tweetnacl": "^1.0.3",
    "url-join": "4.x"
  },
  "devDependencies": {
    "@types/encodeurl": "^1.0.0",
    "@types/node": "^20.19.30",
    "@types/random-seed": "^0.3.3",
    "@types/seed-random": "^2.2.1",
    "@types/url-join": "4.x",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "copyfiles": "^2.4.1",
    "del-cli": "^4.0.1",
    "discord-api-types": "^0.33.5",
    "discordeno": "^13.0.0-rc45",
    "esbuild": "^0.14.43",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^13.1.3",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.1",
    "tsx": "^4.19.4",
    "typescript": "^5.9.3"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
